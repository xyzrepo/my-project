{
  "defaultConnection": "default",
  "connections": {
    "default": {
      "connector": "strapi-hook-bookshelf",
      "settings": {
        "client": "sqlite",
        "filename": "${process.env.DATABASE_STORAGE || '.db/data.db' }"
      },
      "options": {
        "useNullAsDefault": true
      }
    }
  }
}
